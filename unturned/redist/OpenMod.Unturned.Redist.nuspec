<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>OpenMod.Unturned.Redist</id>
    <version>3.24.6.1</version>
    <description>Unturned 3 redistributables for OpenMod. Does not require OpenMod and can be used standalone.</description>
    <authors>SmartlyDressedGames;OpenMod</authors>
    <license type="file">LICENSE.txt</license>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <readme>docs\README.md</readme>
  </metadata>
  <files>
    <file src="*.dll" target="lib\netstandard2.1" />
    <file src="*.xml" target="lib\netstandard2.1" />
    <file src="*.dll" target="lib\net48"/>
    <file src="*.xml" target="lib\net48"/>
    <file src="LICENSE.txt" target="" />
    <file src="README.md" target="docs\" />
  </files>
</package>
