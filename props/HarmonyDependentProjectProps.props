<Project>
  <Target Name="ChangeAliasesOfHarmonyNuGetRefs" BeforeTargets="FindReferenceAssembliesForReferences;ResolveReferences">
    <ItemGroup>
      <ReferencePath Condition="'%(FileName)' == 'MonoMod.ILHelpers'">
        <Aliases>MonoModILHelpers</Aliases>
      </ReferencePath>
      <ReferencePath Condition="'%(FileName)' == 'MonoMod.Backports'">
        <Aliases>MonoModBackports</Aliases>
      </ReferencePath>
    </ItemGroup>
  </Target>
</Project>